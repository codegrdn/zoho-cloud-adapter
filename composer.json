{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "doctrine/dbal": "^2.9",
    "firebase/php-jwt": "^5.0",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/mail": "5.8.*",
    "kunalvarma05/dropbox-php-sdk": "^0.2.1",
    "laravel/lumen-framework": "5.8.*",
    "league/oauth2-client": "^2.6",
    "microsoft/microsoft-graph": "^1.27",
    "mpociot/laravel-apidoc-generator": "3.9.0",
    "vlucas/phpdotenv": "^3.3",
    "zohocrm/php-sdk-archive": "^2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
